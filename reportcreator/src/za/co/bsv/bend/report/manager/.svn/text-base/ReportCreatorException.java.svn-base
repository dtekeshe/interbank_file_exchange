package za.co.bsv.bend.report.manager;

/**
 *
 * @author setumor
 */
public class ReportCreatorException extends Exception {

    public ReportCreatorException() {
        super();
    }
    
    public ReportCreatorException(String msg) {
        super(msg);
    }
    
    public ReportCreatorException(String msg, Throwable cause) {
        super(msg,cause);
    }
    
}
